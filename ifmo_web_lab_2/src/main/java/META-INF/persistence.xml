<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
<mapping-file>META-INF/*</mapping-file>
    <persistence-unit name="postgres">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <class>com.dmitri.ifmo_web_lab_2.dto.HitCheckDTO</class>
        <properties>
            <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
            <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/dmitri"/>
            <propetty name="javax.persistence.jdbc.user" value="dmitri"/>
            <property name="javax.persistence.jdbc.password" value="1234"/>
            <property name="javax.persistence.jdbc.schema-generation.database.action" value="create"/>
    </persistence-unit>
</persistence>
